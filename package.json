{
  "name": "@danielx/hera",
  "version": "0.7.3-pre.1",
  "description": "Small and fast parsing expression grammars",
  "devDependencies": {
    "@danielx/coffeecoverage": "4.0.0-pre.1",
    "@types/node": "^17.0.21",
    "benchmark": "^2.1.4",
    "coffeescript": "^2",
    "microtime": "^3.0.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {},
  "homepage": "https://danielx.net/hera/docs/README.html",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielXMoore/hera.git"
  },
  "mocha": {
    "extension": [
      "coffee"
    ],
    "require": [
      "ts-node/register",
      "coffeescript/register",
      "@danielx/coffeecoverage/register-istanbul",
      "./test/helper.coffee"
    ],
    "reporter": "spec",
    "recursive": true,
    "spec": [
      "test"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".coffee"
    ],
    "include": [
      "source/"
    ],
    "exclude": [
      "lsp/",
      "node_modules/",
      "source/exp/",
      "source/cli.coffee",
      "source/old_main.coffee",
      "source/rules.coffee"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "prepare": "bash script/prepare",
    "test": "nyc mocha"
  },
  "bin": {
    "hera": "bin/hera"
  },
  "files": [
    "dist/",
    "register.js"
  ],
  "main": "dist/main.js"
}
