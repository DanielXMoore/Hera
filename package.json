{
  "name": "@danielx/hera",
  "version": "0.7.12",
  "description": "Small and fast parsing expression grammars",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/register": "^7.17.7",
    "@danielx/civet": "^0.3.15",
    "@danielx/coffeecoverage": "4.0.0-pre.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "babel-plugin-istanbul": "^6.1.1",
    "benchmark": "^2.1.4",
    "coffeescript": "^2",
    "esbuild-coffeescript": "^2.0.0",
    "microtime": "^3.0.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.2"
  },
  "homepage": "https://danielx.net/hera/docs/README.html",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielXMoore/hera.git"
  },
  "mocha": {
    "extension": [
      "coffee"
    ],
    "require": [
      "./test/helper.coffee"
    ],
    "loader": [
      "ts-node/esm",
      "./build/coffee-esm.mjs",
      "./build/hera-esm.mjs",
      "@danielx/civet/esm"
    ],
    "reporter": "spec",
    "recursive": true,
    "spec": [
      "test"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".coffee",
      ".js",
      ".ts"
    ],
    "exclude": [
      "esbuild.coffee",
      "perf/",
      "source/cli.coffee",
      "source/esbuild-plugin.coffee",
      "source/machine.js",
      "source/old_main.coffee",
      "source/parser.js",
      "test/"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "benchmark": "coffee perf/benchmark.coffee",
    "build": "bash build/compile",
    "test": "nyc mocha"
  },
  "bin": {
    "hera": "dist/hera"
  },
  "files": [
    "dist/",
    "esbuild-plugin.js",
    "register.js"
  ],
  "main": "dist/main.js",
  "types": "dist/main.d.ts"
}
