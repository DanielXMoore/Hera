{
  "name": "hera",
  "version": "0.7.1",
  "description": "Small and fast parsing expression grammars",
  "devDependencies": {
    "@danielx/coffeecoverage": "4.0.0-pre.1",
    "coffeescript": "^2",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0"
  },
  "dependencies": {},
  "homepage": "https://danielx.net/hera/docs/README.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/STRd6/hera.git"
  },
  "mocha": {
    "extension": [
      "coffee"
    ],
    "require": [
      "coffeescript/register",
      "@danielx/coffeecoverage/register-istanbul",
      "./test/helper.coffee"
    ],
    "reporter": "spec",
    "recursive": true,
    "spec": [
      "test"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".coffee"
    ],
    "exclude": [
      "source/rules.coffee"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "prepare": "bash script/prepare",
    "test": "nyc mocha"
  },
  "files": [
    "dist/",
    "register.js"
  ],
  "main": "dist/main.js"
}
